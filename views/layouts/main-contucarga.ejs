                        <input class="form-check-input" type="radio" value="exportacion" id="input4_ext" name="tipo_de_operacion">
                        <label class="form-check-label" for="input4_ext">Exportación</label>
                      </div>
                      <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" value="importacion" id="input4_imp" name="tipo_de_operacion">
                        <label class="form-check-label" for="input4_ext">Importación</label>
                      </div>
                      <div class="form-check form-check-inline">
                        <input class="form-check-input" type="radio" value="otro" id="input4_otro" name="tipo_de_operacion">
